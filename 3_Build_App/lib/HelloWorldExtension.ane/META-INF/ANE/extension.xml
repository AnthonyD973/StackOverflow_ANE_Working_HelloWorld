	<!--<platforms> must provide the native library's name, as well as the name of the initializer
	and finalizer, otherwise you will get error #3500.-->
	<platforms>
		<platform name="Windows-x86">
			<applicationDeployment>
				<nativeLibrary>HelloWorld.dll</nativeLibrary>
				<initializer>extensionInitializer</initializer>	<!--Name must NOT contain an underscore-->
				<finalizer>extensionFinalizer</finalizer>		<!--Name must NOT contain an underscore-->
			</applicationDeployment>
		</platform>
	</platforms>
	
</extension>